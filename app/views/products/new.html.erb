<div class="row">
	<div class="col-md-12">
		<div class="row">
			<div class="col-md-12">
				<h1>New Product</h1>
			</div>	
		</div>
		<div class="row">
			<div class="col-md-12">
				<form class="form-horizontal" style="margin-left: -59px;">
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Product name</label>
					    <div class="col-sm-6">
					      <input type="text" class="form-control" id="" placeholder="Product name">
					    </div>
					</div>
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Price</label>
					    <div class="col-sm-6">
					      <input type="text" class="form-control" id="" placeholder="Price">
					    </div>
					</div>
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Min. order</label>
					    <div class="col-sm-6">
					      <input type="text" class="form-control" id="" placeholder="Min. order">
					    </div>
					</div>
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Origin</label>
					    <div class="col-sm-6">
					      <input type="text" class="form-control" id="" placeholder="Origin">
					    </div>
					</div>
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Description</label>
					    <div class="col-sm-6">
					      <textarea class="form-control"></textarea> 
					    </div>
					</div>
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Dimension</label>
					    <div class="col-sm-2">
					      <input type="text" class="form-control" id="" placeholder="height">
					    </div>
					    <div class="col-sm-2">
					      <input type="text" class="form-control" id="" placeholder="widht">
					    </div>
					    <div class="col-sm-1">
					      <input type="text" class="form-control" id="" placeholder="depth">
					    </div>
					    <div class="col-sm-1">
					      <select class="form-control">
					      	<option selected>cm</option>
					      	<option selected>m</option>
					      	<option selected>inch</option>
					      </select>
					    </div>
					</div>
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Material</label>
					    <div class="col-sm-6">
					      <input type="text" class="form-control" id="" placeholder="Material">
					    </div>
					</div>
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Weight</label>
					    <div class="col-sm-5">
					      <input type="text" class="form-control" id="" placeholder="weight">
					    </div>
					    <div class="col-sm-1">
					      <!--<input type="text" class="form-control" id="" placeholder="unit">-->
					      <select class="form-control">
					      	<option selected>kg</option>
					      	<option selected>g</option>
					      	<option selected>ton</option>
					      	<option selected>pound</option>
					      	<option selected>ounch</option>
					      </select>
					    </div>
					 </div>
					<div class="form-group">
					    <label for="inputEmail3" class="col-sm-2 control-label">Color</label>
					    <div class="col-sm-6">
					      <input type="text" class="form-control" id="" placeholder="Color">
					    </div>
					</div>
					<div class="form-group">
						<div class="col-xs-offset-1 col-xs-8">
							<button type="button" class="btn btn-primary">Sign Up</button>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>

