<div class="row content user-form-page">
	<div class="col-md-5">
		<div class="row user-h">
			<h2>Sign Up</h2>
		</div>
		<div class="row" style="margin-top:20px;">
			<form class="form-horizontal">
				<div class="form-group">
					<label for="Contact_name" class="control-label col-xs-3">Contact name</label>
					<div class="col-xs-3">
						<input type="text" class="form-control user-form-name" id="first_name" placeholder="firstname">
					</div>
					<div class="col-xs-3" style="margin-left: -14px;">
						<input type="text" class="form-control user-form-name" id="Last_name" placeholder="Lastname">
					</div>
				</div>
				<div class="form-group">
					<label for="Username" class="control-label col-xs-3">Username</label>
					<div class="col-xs-8">
						<input type="text" class="form-control user-form" id="username" placeholder="username">
					</div>
				</div>
				<div class="form-group">
					<label for="Email" class="control-label col-xs-3">Email</label>
					<div class="col-xs-8">
						<input type="email" class="form-control user-form" id="Email" placeholder="email">
					</div>
				</div>
				<div class="form-group">
					<label for="Password" class="control-label col-xs-3">Password</label>
					<div class="col-xs-8">
						<input type="Password" class="form-control user-form" id="Email" placeholder="Password">
					</div>
				</div>
				<div class="form-group">
					<label for="Re-Password" class="control-label col-xs-3">Re-Password</label>
					<div class="col-xs-8">
						<input type="Password" class="form-control user-form" id="Email" placeholder="Re-Password">
					</div>
				</div>
				<div class="form-group">
					<label for="Sign up as" class="control-label col-xs-3">Sign up as</label>
					<div class="col-xs-8">
						<span class="input-group">
							<input type="radio" name="type" id="sign_up_radio">buyer</input>
							<input type="radio" name="type" id="sign_up_radio">seller</input>
						</span>
					</div>
				</div>
				<div class="form-group">
					<div class="col-xs-offset-3 col-xs-8">
						<button type="button" class="btn btn-primary signup-button">Sign Up</button>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>
